CONSTRUCT {
?fireInstance :locatedIn ?section1.
?fireInstance :atTime ?time1.
}
FROM STREAM <http://www.ia.urjc.es/ontologies/building/sbevac/SmokeEvents/SmokeDetected/> [RANGE 1s TUMBLING]
FROM STREAM <http://www.ia.urjc.es/ontologies/building/sbevac/TemperatureEvents/TemperatureRisen/> [RANGE 1s TUMBLING]
FROM <http://www.ia.urjc.es/ontologies/building/sbevac_scenario>
WHERE {

?SmokeID a :SmokeSensor.
?SmokeID :installedIn ?section1.
?SmokeID :atTime ?time1.

?TempID a :TemperatureSensor.
?TempID	:installedIn ?section1.
?TempID :atTime ?time1.

BIND(IRI(CONCAT(STR(?section1), "Fire")) AS ?fireInstance )

}