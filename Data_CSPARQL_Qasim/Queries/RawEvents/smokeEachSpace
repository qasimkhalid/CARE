CONSTRUCT {
?SmokeID :hasSmoke ?value1.
?SmokeID :atTime ?time1.
}
FROM STREAM <http://www.ia.urjc.es/ontologies/building/sbevac/SmokeEvents/> [RANGE 2s TUMBLING]
FROM <http://www.ia.urjc.es/ontologies/building/sbevac_scenario>
WHERE {
?event1 ?SmokeID ?value1.
?SmokeID a sbevac:SmokeSensor.
BIND (f:timestamp(?event1,?SmokeID,?value1) AS ?time1)
}